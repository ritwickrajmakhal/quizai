    "public": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::public.public",
      "inversedBy": "quizzes"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "MCQ",
        "MSQ",
        "NAT",
        "SAQ",
        "LAQ"
      ],
      "required": true,
      "default": "MCQ"
    },
    "questions": {
      "type": "json",
      "required": true
    }
  }
