{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "singularName": "quiz",
    "pluralName": "quizzes",
    "displayName": "quiz",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "difficultyLevel": {
      "type": "enumeration",
      "enum": [
        "easy",
        "medium",
        "hard"
      ]
    },
    "attempts": {
      "type": "relation",
